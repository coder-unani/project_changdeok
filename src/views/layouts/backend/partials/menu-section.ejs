<div class="mb-2 border-t border-border-1">
  <p class="orb-remark p-2 mt-2"><%=title%></p>
  <ul class="flex flex-col gap-2 font-medium text-gray-500 dark:text-gray-100">
    <% items.forEach(function(item) { %>
      <li class="rounded-lg transition-all dark:hover:text-white dark:hover:bg-gray-600 <%if (item.isActive) {%>orb-hightlight<%} else {%>hover:bg-gray-100<%}%>">
        <a class="flex items-center justify-start p-2" href="<%=item.url%>" <%if (item.isExternal) {%> target="_blank"<%}%>>
          <span class="text-left">
            <%- include(`icons/${item.icon}`) %>
          </span>
          <span class="mx-3"><%=item.title%></span>
        </a>
      </li>
    <% }); %>
  </ul>
</div> 