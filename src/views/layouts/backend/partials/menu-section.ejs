<div class="mb-2 border-t border-slate-100">
  <p class="text-xs font-normal text-gray-400 p-2 mt-2"><%=title%></p>
  <ul class="flex flex-col gap-2 text-gray-500 dark:text-gray-100">
    <% items.forEach(function(item) { %>
      <li class="font-thin rounded-lg transition-all dark:hover:text-white dark:hover:bg-gray-600 <%if (item.isActive) {%>orb-hightlight<%} else {%>hover:bg-gray-100<%}%>">
        <a class="flex items-center justify-start p-2" href="<%=item.url%>">
          <span class="text-left">
            <%- include(`icons/${item.icon}`) %>
          </span>
          <span class="mx-3 font-normal"><%=item.title%></span>
        </a>
      </li>
    <% }); %>
  </ul>
</div> 