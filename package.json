{
  "name": "express_base",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "dev": "npm-run-all --parallel nodemon sass tailwind",
    "build": "tsc && npm run tailwind:build && npm run copy-files",
    "start": "node dist/app.js",
    "sass": "sass --watch src/styles:public/css",
    "tailwind": "npx @tailwindcss/cli -i src/styles/tailwind.css -o public/css/tailwind.css --watch",
    "tailwind:build": "npx @tailwindcss/cli -i src/styles/tailwind.css -o public/css/tailwind.css",
    "copy-files": "cp -R src/views dist/views && cp -R public dist/static",
    "nodemon": "nodemon",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "author": "coder-unani",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.4.1",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
